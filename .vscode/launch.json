{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Push Swap",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/push_swap", 
            "args": [5 4 8 9 7 6 2 1 3 0],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build with Makefile"
        }
    ]
}
